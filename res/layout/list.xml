<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/rank" android:textSize="17sp" android:width="45dp"></TextView>
<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/count" android:textSize="17sp" android:width="45dp"></TextView>
<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/stage" android:textSize="17sp" android:width="90dp"></TextView>
